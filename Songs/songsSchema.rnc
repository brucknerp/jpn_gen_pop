start = song
song = element song {meta, body}
meta = element meta {title+,singer+,lyricist+,release+}
title = element title {text,language}
language = attribute language {"japanese"|"english"}
singer = element singer {text,language,gender}
lyricist = element lyricist {text,language,gender}
gender = attribute gender {"male"|"female"|"mixed"}
release = element release {text}
body = element body {verse+&bridge*&chorus*}
verse = element verse {line+}
bridge = element bridge {line+}
chorus = element chorus {line+}
line = element line {mixed{(insert)*}}
insert = element insert {text}

